check process CoExpression with pidfile /kb/deployment/services/CoExpression/service.pid
  start program = "/kb/deployment/services/CoExpression/start_service" with timeout 60 seconds
  stop  program = "/kb/deployment/services/CoExpression/stop_service"
  if failed port 7063 type tcp 
     with timeout 15 seconds
     then restart
  if 3 restarts within 5 cycles then timeout
  group CoExpression_group
